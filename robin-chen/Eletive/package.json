{
  "name": "spotstress",
  "version": "0.1.0",
  "description": "SPOTstress application by Eletive",
  "main": "source/index.js",
  "scripts": {
    "test": "cross-env STAGE=test jest",
    "test:watch": "cross-env STAGE=test jest --watch --verbose",
    "test:coverage": "cross-env STAGE=test jest --coverage --colors",
    "test:ci": "cross-env STAGE=test JEST_JUNIT_OUTPUT_DIR=./test-reports jest --coverage --reporters=jest-junit",
    "start": "npm run startDevAPI",
    "start:hot": "cross-env STAGE=dev npm run startProdAPI -- --hot",
    "startCI": "http-server dist --silent",
    "startProdAPI": "cross-env NODE_ENV=development webpack-dev-server --progress --colors",
    "startDevAPI": "cross-env STAGE=dev npm run startProdAPI",
    "startLocalAPI": "cross-env STAGE=localAPI npm run startProdAPI",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "buildDev": "cross-env STAGE=dev npm run build",
    "maintanenceProd": "aws --profile eletive s3 rm s3://prod.eletive.com/ --recursive && aws --profile eletive s3 cp ./public/maintenance/ s3://prod.eletive.com/ --recursive && aws --profile eletive cloudfront create-invalidation --distribution-id E134C2B0267U3L --paths '/*'",
    "deployDev": "npm run buildDev && aws --profile eletive s3 rm s3://dev.eletive.com/ --recursive && aws --profile eletive s3 cp ./dist s3://dev.eletive.com/ --recursive",
    "deployProd": "npm run build && aws --profile eletive s3 sync ./dist s3://prod.eletive.com/ --size-only --exclude 'index.html' && aws --profile eletive s3 sync ./dist s3://prod.eletive.com/ --delete && aws --profile eletive cloudfront create-invalidation --distribution-id E134C2B0267U3L --paths '/*'",
    "eslint": "eslint ./source/ --ext .js,.jsx",
    "ngrok": "node ./scripts/ngrok",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cypress:open": "cypress open",
    "e2e": "cypress run",
    "e2e:ci": "cypress run --browser chrome --env STAGE=dev",
    "e2e:record": "cypress run --record",
    "deployDevPptxTemplates": "aws --profile eletive s3 cp ./PowerpointTemplates s3://images.eletive.com/dev/reports/templates/ --recursive && aws --profile eletive cloudfront create-invalidation --distribution-id E24XN73I5TCI7U --paths '/dev/reports/templates/*'",
    "deployProdPptxTemplates": "aws --profile eletive s3 cp ./PowerpointTemplates s3://images.eletive.com/prod/reports/templates/ --recursive && aws --profile eletive cloudfront create-invalidation --distribution-id E24XN73I5TCI7U --paths '/prod/reports/templates/*'"
  },
  "sideEffects": [
    "./source/styles/index.scss"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:eletive/dashboard.git"
  },
  "author": "Eletive",
